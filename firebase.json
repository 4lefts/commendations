{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "**/src/**",
      "**/.*",
      "**/node_modules/**",
      "firebase.json",
      "gulpfile.js",
      "package.json",
      "README.md"
    ]
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
